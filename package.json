{
  "name": "nodeserver",
  "version": "1.0.0",
  "description": "sending data to react using express and node server",
  "main": "index.js",
  "scripts": {
    "client-install": "cd contactlist && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix contactlist",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "ashish singh",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.3"
  }
}
